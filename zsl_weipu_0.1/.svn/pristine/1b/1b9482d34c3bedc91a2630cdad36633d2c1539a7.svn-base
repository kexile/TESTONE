/**
 * 编辑工具
 */
class EditUtils {

    /**
     * 编辑文字
     */
    static editText(e){
        var that = this
        wx.navigateTo({
            url: "/pages/common/edit/edit" + "?name=" + e.currentTarget.dataset.name + "&value=" + e.currentTarget.dataset.value
        })
    }
    
}

export default EditUtils